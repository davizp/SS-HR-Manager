<!-- Modal -->
<div class="modal fade" id="modal_tipos_permisos" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
				<h4 class="modal-title" id="modal_tipos_permisos_titulo" ></h4>
			</div>
			<div class="modal-body">
				<!-- FORMS ================================== -->
				<form role="form" id="form_tipos_permisos" class="form-horizontal well" method="post" action="/tipos_permisos-data">
					<div class="form-group">
						<label for="tipo_permiso">Nombre del Tipo de Permiso:</label>
						<input type="text" name="tipo_permiso" class="form-control" placeholder="Permiso" onblur = "alerta($(this))" required>
					</div>
				</form>
				<!--/FORMS ================================== -->
			</div>
			<div class="modal-footer">
				<button id = "guardar" type="button" class="btn btn-danger" id="confirm" >Guardar</button>
				<button type="button" class="btn btn-default" data-dismiss="modal">Cancelar</button>
			</div>
		</div>
	</div>
</div>
